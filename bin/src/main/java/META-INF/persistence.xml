<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
	<persistence-unit name="WassabiApp" transaction-type="RESOURCE_LOCAL">
	<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
	<class>com.wassabi.model.Cartao</class>
	<class>com.wassabi.model.CartaoId</class>
	<class>com.wassabi.model.Categoria</class>
	<class>com.wassabi.model.Cliente</class>
	<class>com.wassabi.model.Endereco</class>
	<class>com.wassabi.model.EnderecoId</class>
	<class>com.wassabi.model.Produto</class>
	<class>com.wassabi.model.Venda</class>
	<class>com.wassabi.model.VendaHasProduto</class>
	<class>com.wassabi.model.VendaHasProdutoId</class>
	<properties>
			<property name="hibernate.cfg_xml_file" value="META-INF/hibernate.cfg.xml"/>
	</properties>
	</persistence-unit>
</persistence>
